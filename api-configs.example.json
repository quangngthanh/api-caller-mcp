{
  "custom_api": {
    "name": "custom_api",
    "baseUrl": "https://your-api.com/v1",
    "headers": {
      "Content-Type": "application/json",
      "User-Agent": "MCP-API-Caller/1.0"
    },
    "authentication": {
      "type": "api_key",
      "apiKey": "your_api_key_here",
      "headerName": "X-API-Key"
    }
  }
}